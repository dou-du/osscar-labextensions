{
  "name": "osscar-labextensions",
  "version": "0.1.0",
  "description": "A collection of JupyterLab extensions for OSSCAR.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "git@github.com:dou-du/osscar-labextensions.git",
  "bugs": {
    "url": "git@github.com:dou-du/osscar-labextensions.git/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Dou Du",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "git@github.com:dou-du/osscar-labextensions.git.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0-beta.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
